		sortedBankAccount=new TreeSet<BankAccount>(new BankAccountFullNameComparator());
		sortedBankAccount.add(new BankAccount("Mangesh","123",99999999999.99));
		sortedBankAccount.add(new BankAccount("Aman","234",999999999.99));
		sortedBankAccount.add(new BankAccount("Aaditya","345",99999999.99));
		sortedBankAccount.add(new BankAccount("Akshansh","456",9999999999.99));
		
		for(BankAccount i:sortedBankAccount){
			System.out.println(i.getFullName()+i);
		}
		
		
		System.out.println("\n-----------------------------------------------\n");
		
		Set<BankAccount> lhs=new LinkedHashSet<BankAccount>();
		
		lhs.add(new BankAccount("QQQ","1234",1523.25));
		lhs.add(new BankAccount("WWW","12345",153.25));
		lhs.add(new BankAccount("EEE","123456",523.25));
		
		for(BankAccount b:lhs){
			System.out.println(b.getFullName()+b);
		}
		
		System.out.println("\n-----------------------------------------------\n");
		
		Map<String,BankAccount> m = new HashMap<String,BankAccount>();
		
		m.put("123",new BankAccount());
		m.put("13",new BankAccount());
		m.put("23",new BankAccount());
		
		for(Map.Entry<String, BankAccount>ac :m.entrySet()){
			System.out.println(ac.getKey());
			System.out.println(ac.getValue());
		}
		
		Map<Aadhar,List<BankAccount>> data;
		
		
		
	}
	
