package com.psl.main;

import java.util.HashSet;
import java.util.SortedSet;
import java.util.TreeSet;

import com.psl.util.BankAccount;
import com.psl.util.BankAccountFullNameComparator;

public class UserInteface {
	static HashSet<BankAccount> acs;
	public static void main(String[] args) {
		acs=new HashSet<BankAccount>();
		
		acs.add(new BankAccount("1234",22.12));
		acs.add(new BankAccount("12344",222.12));
		acs.add(new BankAccount("12344",2112.12));
		acs.add(new BankAccount("12345",22.12));
		
		System.out.println(acs.size());
		
		for(BankAccount a:acs){
			System.out.println(a);
		}
		
		System.out.println("\n-----------------------------------------------\n");
		SortedSet<BankAccount> sortedBankAccount;
		
		sortedBankAccount = new TreeSet<BankAccount>(acs); 
		for(BankAccount i:sortedBankAccount){
			System.out.println(i);
		}
		
		System.out.println("\n-----------------------------------------------\n");
		
		sortedBankAccount=new TreeSet<BankAccount>(new BankAccountFullNameComparator());
		sortedBankAccount.add(new BankAccount("Mangesh","123",99999999999.99));
		sortedBankAccount.add(new BankAccount("Aman","234",999999999.99));
		sortedBankAccount.add(new BankAccount("Aaditya","345",99999999.99));
		sortedBankAccount.add(new BankAccount("Akshansh","456",9999999999.99));
		
		for(BankAccount i:sortedBankAccount){
			System.out.println(i.getFullName()+i);
		}
		
		
		
		
		
		
		
	}
}
